[Desktop Entry]
Version=1.0
Type=Application
Name=Personalize
Name[pt_BR]=Personalizar
Name[es]=Personalizar
Name[fr]=Personnaliser
Name[de]=Personalisieren
GenericName=Appearance Settings
GenericName[pt_BR]=Configuracoes de Aparencia
Comment=Customize themes, layouts, and desktop experience
Comment[pt_BR]=Personalize temas, layouts e a experiencia do desktop
Comment[es]=Personalizar temas, disenos y experiencia de escritorio
Comment[fr]=Personnaliser les themes, mises en page et experience bureau
Comment[de]=Themes, Layouts und Desktop-Erfahrung anpassen
Keywords=theme;style;windows;mac;linux;personalize;appearance;wallpaper;icons;fonts;colors;dark;light;
Keywords[pt_BR]=tema;estilo;windows;mac;linux;personalizar;aparencia;papel de parede;icones;fontes;cores;escuro;claro;
Exec=/usr/bin/winux-personalize
Icon=preferences-desktop-theme
Terminal=false
Categories=System;Settings;DesktopSettings;X-Winux;
StartupNotify=true
StartupWMClass=winux-personalize
X-GNOME-SingleWindow=true
X-KDE-StartupNotify=true

Actions=windows-style;mac-style;linux-style;

[Desktop Action windows-style]
Name=Apply Windows Style
Name[pt_BR]=Aplicar Estilo Windows
Exec=/usr/bin/winux-personalize --apply-windows
Icon=windows

[Desktop Action mac-style]
Name=Apply macOS Style
Name[pt_BR]=Aplicar Estilo macOS
Exec=/usr/bin/winux-personalize --apply-mac
Icon=apple

[Desktop Action linux-style]
Name=Apply Linux Style
Name[pt_BR]=Aplicar Estilo Linux
Exec=/usr/bin/winux-personalize --apply-linux
Icon=start-here
