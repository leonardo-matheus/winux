<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">

  <!-- Winux Project Files -->
  <mime-type type="application/x-winux-project">
    <comment>Winux Builder Project</comment>
    <comment xml:lang="pt_BR">Projeto do Winux Builder</comment>
    <icon name="winux-builder"/>
    <glob pattern="*.winux-project"/>
    <glob pattern="*.wxp"/>
  </mime-type>

  <mime-type type="application/x-winux-config">
    <comment>Winux Configuration File</comment>
    <comment xml:lang="pt_BR">Arquivo de Configuracao Winux</comment>
    <icon name="preferences-system"/>
    <glob pattern="*.winux-config"/>
    <glob pattern="*.wxc"/>
  </mime-type>

  <mime-type type="application/x-winux-theme">
    <comment>Winux Theme Package</comment>
    <comment xml:lang="pt_BR">Pacote de Tema Winux</comment>
    <icon name="preferences-desktop-theme"/>
    <glob pattern="*.winux-theme"/>
    <glob pattern="*.wxt"/>
  </mime-type>

  <!-- Source Code Files - Associated with winux-edit -->
  <mime-type type="text/x-python">
    <comment>Python Script</comment>
    <icon name="text-x-python"/>
    <glob pattern="*.py"/>
    <glob pattern="*.pyw"/>
    <glob pattern="*.pyx"/>
  </mime-type>

  <mime-type type="text/x-csrc">
    <comment>C Source Code</comment>
    <icon name="text-x-csrc"/>
    <glob pattern="*.c"/>
    <glob pattern="*.h"/>
  </mime-type>

  <mime-type type="text/x-c++src">
    <comment>C++ Source Code</comment>
    <icon name="text-x-c++src"/>
    <glob pattern="*.cpp"/>
    <glob pattern="*.cxx"/>
    <glob pattern="*.cc"/>
    <glob pattern="*.hpp"/>
    <glob pattern="*.hxx"/>
  </mime-type>

  <mime-type type="application/javascript">
    <comment>JavaScript Source</comment>
    <icon name="text-x-javascript"/>
    <glob pattern="*.js"/>
    <glob pattern="*.mjs"/>
    <glob pattern="*.cjs"/>
  </mime-type>

  <mime-type type="application/typescript">
    <comment>TypeScript Source</comment>
    <icon name="text-x-typescript"/>
